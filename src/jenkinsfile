pipeline {
    agent {
        label 'nop'
    }
    triggers {
        pollSCM('* * * * *')
    }
    stages {
        satage('scm') {
            steps {
                git url: 'https://github.com/rajkumar1805/nopCommerceapr24',
                    branch: 'develop'
            }

        }
        stage ('build') {
             steps {
                sh 'dotnet publish -0 published/ release -c src/presentation/nop.web/nop.web.csproj'
             } 
        }       
        
    }
}
